{
    "name": "newebpay-project/newebpay-project",
    "description": "A simple project.",
    "require": {
        "vlucas/phpdotenv": "^5.4",
        "guzzlehttp/guzzle": "^7.0"  
    },
    "autoload": {
        "psr-4": {
            "YourNamespace\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true
    }
}
